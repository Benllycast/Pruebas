....................   i2c_start(); 
....................   i2c_write(0xD0); 
....................   i2c_write(0x00); 
....................   i2c_start(); 
....................   i2c_write(0xD1); 
....................   seconds = ds1307_bcd2bin(i2c_read(0)); 
....................   i2c_stop(); 
....................   seconds &= 0x7F; 
....................  
....................   delay_us(3); 
....................  
....................   i2c_start(); 
....................   i2c_write(0xD0); 
....................   i2c_write(0x00); 
....................   i2c_write(ds1307_bin2bcd(seconds)); 
....................   i2c_start(); 
....................   i2c_write(0xD0); 
....................   i2c_write(0x07); 
....................   i2c_write(val); 
....................   i2c_stop(); 
....................  
.................... #ifndef USE_INTERRUPTS 
....................   enable_interrupts(global); 
.................... #endif 
....................  
.................... } 
