....................  
....................    if(!MEMORIA_OK)return(-5); 
....................    if(MEM_proceso != SET) return(-6); 
....................    if(tamano <= 0)return(-7); 
....................  
....................    i = 0; 
....................    while((tamano > 0)&&(i < size)){ 
....................       MEMORIA_putc(data[i]); 
....................       i++; 
....................       tamano--; 
....................    } 
....................     
....................    //si no hay mas datos 
....................    if(tamano <= 0){ 
....................       MEM_RESPONSE = MEMORIA_getc(); 
