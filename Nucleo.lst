.................... int1 ext_eeprom_ready() { 
....................   int1 ack; 
....................   i2c_start();            // If the write command is acknowledged, 
....................   ack = i2c_write(0xa0);  // then the device is ready. 
....................   i2c_stop(); 
....................   return !ack; 
.................... } 
.................... #endif 
