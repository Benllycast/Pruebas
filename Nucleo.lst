....................    strcpy (MEM_file_name,""); 
....................    i = 0; 
....................    car = 0; 
....................    timeout_error = FALSE; 
....................    MEM_handshaking = MEMORIA_DEFAULT_HANDSHAKING; 
....................    MEM_RESPONSE = MEMORIA_NOACK; 
....................    MEM_proceso = OPEN; 
....................    return(0); 
.................... } 
