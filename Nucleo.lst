.................... /*==================== obtiene un caracter del bus======================*/ 
.................... char MEMORIA_getc(void){ 
.................... 	char c = 0x00, cont = 3; 
....................    timeout_error=FALSE; 
....................    while(!kbhit(MEMORIA) && cont > 0 && !c){ 
....................    	c = fgetc(MEMORIA); 
....................    	#ifdef debug_memoria 
....................    	usb_cdc_putc_fast('*'); 
....................    	#endif 
....................    	cont--; 
....................    } 
....................     
....................    if(!c) c = fgetc(MEMORIA); 
....................     
....................    #ifdef debug_memoria 
....................    printf(usb_cdc_putc_fast,"\n\r<%x e%x", c, rs232_errors); 
....................    #endif 
....................    return(c); 
.................... } 
