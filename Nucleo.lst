....................    fputc(make8(tamano,3),MEMORIA); 
....................    fputc(make8(tamano,2),MEMORIA); 
....................    fputc(make8(tamano,1),MEMORIA); 
....................    fputc(make8(tamano,0),MEMORIA); 
....................  
....................    MEM_RESPONSE = MEMORIA_getc(); 
....................    if((MEM_RESPONSE == 0) && timeout_error) 
