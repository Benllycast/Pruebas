.................... int AD_leer_canal(int canal, long *buffer){ 
.................... 	set_adc_channel(canal); 
.................... 	read_adc(ADC_START_ONLY); 
.................... 	int1 done = adc_done(); 
.................... 	while(!done){ 
.................... 		done = adc_done(); 
.................... 	} 
.................... 	*buffer = read_adc(ADC_READ_ONLY); 
.................... 	return 0; 
.................... } 
